    "lodash-es": "^4.17.7",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-dev-utils": "^5.0.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-slick": "^0.21.0",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.2",
    "seamless-immutable": "^7.1.3",
    "style-loader": "0.19.0",
    "subscriptions-transport-ws": "^0.9.7",
